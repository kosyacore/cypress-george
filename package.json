{
  "name": "cypress-george",
  "version": "1.0.0",
  "description": "Cypress framework for George assignment",
  "main": "index.js",
  "scripts": {
    "test": "npm run report:clear && npm run cy:run:chrome && npm run report:open",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "cy:run:chrome": "cypress run --browser chrome",
    "cy:run:electron": "cypress run --browser electron",
