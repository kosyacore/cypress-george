    "report:open": "open ./reports/cucumber_report.html"
  },
  "author": "Anton Klimko",
  "devDependencies": {
    "@badeball/cypress-cucumber-preprocessor": "^15.1.4",
    "@bahmutov/cypress-esbuild-preprocessor": "^2.2.0",
    "cypress": "^12.7.0",
    "esbuild": "^0.17.10",
    "typescript": "^4.9.5"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:kosyacore/cypress-george.git"
  }
}